# Configurações do PostgreSQL
POSTGRES_HOST=your-postgres-host
POSTGRES_PORT=5432
POSTGRES_DB=speedfunnels
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password

# Configurações do Redis
REDIS_HOST=your-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# Configurações do JWT
JWT_SECRET=your-jwt-secret-key-at-least-32-characters-long

# Configurações do Facebook/Meta
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret
META_APP_ID=your-meta-app-id
META_APP_SECRET=your-meta-app-secret

# Configurações do Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Configurações de Admin
SUPER_ADMIN_EMAIL=admin@example.com
SUPER_ADMIN_PASSWORD=your-admin-password
MIGRATION_SECRET_KEY=your-migration-secret-key

# Configurações de domínio
API_DOMAIN=api.yourdomain.com
FRONTEND_DOMAIN=app.yourdomain.com

# Configurações de CORS
# Lista de domínios separados por vírgula que podem acessar a API
CORS_ALLOWED_ORIGINS=https://app.yourdomain.com,http://localhost:3000
